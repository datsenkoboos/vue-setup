{
  "name": "vue-setup",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "preview": "vite preview",
    "preview:host": "vite preview --host",
    "build": "vite build",
    "build:sb-tailwind": "pnpm exec tailwindcss -i ./src/assets/styles/main.css -o .storybook/compiled.css --minify",
    "build:sb-tailwind:regression": "pnpm exec tailwindcss -i ./src/assets/styles/main.css -o .storybook-regression/compiled.css --minify",
    "build:sb": "pnpm build:sb-tailwind && storybook build",
    "build:sb:regression": "pnpm build:sb-tailwind:regression && storybook build -c ./.storybook-regression",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint --fix .",
    "lint:inspect": "pnpx @eslint/config-inspector --config eslint.config.js",
    "check": "pnpm typecheck && pnpm lint",
    "dev": "vite",
    "dev:sb": "conc \"storybook dev -p 6006 --no-open\" \"pnpm build:sb-tailwind --watch\"",
    "dev:sb:regression": "conc \"storybook dev -p 6006 --no-open -c ./.storybook-regression\" \"pnpm build:sb-tailwind:regression --watch\"",
    "test:unit": "vitest",
    "test:regression": "pnpm build:sb:regression && LOST_PIXEL_DISABLE_TELEMETRY=1 lost-pixel",
    "test:regression:update": "pnpm build:sb:regression && LOST_PIXEL_DISABLE_TELEMETRY=1 lost-pixel update"
  },
  "nano-staged": {
    "*.{vue,ts,js,css,json}": "eslint --fix"
  },
  "dependencies": {
    "@sentry/vue": "^10.1.0",
    "@tanstack/vue-query": "^5.83.1",
    "@vueuse/components": "^13.6.0",
    "@vueuse/core": "^13.6.0",
    "axios": "^1.11.0",
    "lodash-es": "^4.17.21",
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "4.1.0",
    "@faker-js/faker": "^9.9.0",
    "@pinia/testing": "^1.0.2",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^9.1.1",
    "@storybook/blocks": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@storybook/vue3": "^9.1.1",
    "@storybook/vue3-vite": "^9.1.1",
    "@tailwindcss/cli": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/vue-query-devtools": "^5.84.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/user-event": "^14.6.1",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node20": "^20.1.6",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.2.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "eslint": "^9.32.0",
    "eslint-config-fans": "^1.2.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lost-pixel": "^3.22.0",
    "mockdate": "^3.0.5",
    "msw": "^2.10.4",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^8.0.4",
    "storybook": "^9.1.1",
    "storybook-vue3-router": "^6.0.2",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.9.2",
    "vite": "^7.0.6",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.5"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "packageManager": "pnpm@10.8.1+sha512.c50088ba998c67b8ca8c99df8a5e02fd2ae2e2b29aaf238feaa9e124248d3f48f9fb6db2424949ff901cffbb5e0f0cc1ad6aedb602cd29450751d11c35023677"
}
