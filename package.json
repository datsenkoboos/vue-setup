{
  "name": "vue-setup",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "preview": "vite preview --host",
    "preview:local": "vite preview",
    "build": "vite build",
    "build:sb-scss": "sass ./src/assets/styles/main.scss .storybook/main.css",
    "build:sb-tailwind": "pnpm build:sb-scss && pnpm exec tailwindcss -i .storybook/main.css -o .storybook/compiled.css --minify",
    "build:sb": "pnpm build:sb-tailwind && storybook build",
    "check:type": "vue-tsc --noEmit -p tsconfig.vitest.json",
    "check:lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --max-warnings=0",
    "check:format": "prettier --check .",
    "check": "pnpm check:type && pnpm check:lint && pnpm check:format",
    "dev:local": "vite",
    "dev": "vite --host",
    "dev:sb-scss": "pnpm build:sb-scss --watch",
    "dev:sb-tailwind": "pnpm build:sb-tailwind --watch",
    "dev:sb-styles": "conc \"pnpm dev:sb-scss\" \"pnpm dev:sb-tailwind\"",
    "dev:sb": "conc \"storybook dev -p 6006 --no-open\" \"pnpm dev:sb-styles\"",
    "format": "prettier --write --list-different .",
    "test": "vitest"
  },
  "nano-staged": {
    "*.{js,jsx,ts,tsx,vue,css,scss,json,yml}": "prettier --write"
  },
  "dependencies": {
    "@sentry/vue": "^7.119.0",
    "@tanstack/vue-query": "^5.56.2",
    "@vueuse/components": "^10.11.1",
    "@vueuse/core": "^10.11.1",
    "axios": "^1.7.7",
    "lodash-es": "^4.17.21",
    "pinia": "^2.2.2",
    "vue": "^3.5.6",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "1.3.3",
    "@faker-js/faker": "^8.4.1",
    "@pinia/testing": "^0.1.5",
    "@rushstack/eslint-patch": "^1.10.4",
    "@storybook/addon-essentials": "^8.3.1",
    "@storybook/addon-interactions": "^8.3.1",
    "@storybook/addon-links": "^8.3.1",
    "@storybook/blocks": "^8.3.1",
    "@storybook/test": "^8.3.1",
    "@storybook/vue3": "^8.3.1",
    "@storybook/vue3-vite": "^8.3.1",
    "@tanstack/vue-query-devtools": "^5.56.2",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.16.5",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vue": "^9.28.0",
    "husky": "^9.1.6",
    "jsdom": "^24.1.3",
    "msw": "^2.4.8",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^6.2.3",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "sass": "^1.78.0",
    "storybook": "^8.3.1",
    "storybook-vue3-router": "^5.0.0",
    "tailwindcss": "^3.4.11",
    "typescript": "~5.4.5",
    "vite": "^5.4.6",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.1.6"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
