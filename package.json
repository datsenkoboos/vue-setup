{
  "name": "vue-setup",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "preview": "vite preview --host",
    "preview:local": "vite preview",
    "build": "vite build",
    "build:sb-tailwind": "pnpm exec tailwindcss -i ./src/assets/styles/main.css -o .storybook/compiled.css --minify",
    "build:sb": "pnpm build:sb-tailwind && storybook build",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint --fix .",
    "check": "pnpm typecheck && pnpm lint",
    "dev": "vite",
    "dev:sb": "conc \"storybook dev -p 6006 --no-open\" \"pnpm build:sb-tailwind --watch\"",
    "test": "vitest"
  },
  "nano-staged": {
    "*.{vue,ts,js,css,json}": "eslint --fix"
  },
  "dependencies": {
    "@sentry/vue": "^7.119.0",
    "@tanstack/vue-query": "^5.56.2",
    "@vueuse/components": "^10.11.1",
    "@vueuse/core": "^10.11.1",
    "axios": "^1.7.7",
    "lodash-es": "^4.17.21",
    "pinia": "^2.2.2",
    "vue": "^3.5.6",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "1.3.3",
    "@eslint/compat": "^1.1.1",
    "@faker-js/faker": "^8.4.1",
    "@html-eslint/eslint-plugin": "^0.27.0",
    "@html-eslint/parser": "^0.27.0",
    "@pinia/testing": "^0.1.5",
    "@rushstack/eslint-patch": "^1.10.4",
    "@storybook/addon-essentials": "^8.3.1",
    "@storybook/addon-interactions": "^8.3.1",
    "@storybook/addon-links": "^8.3.1",
    "@storybook/blocks": "^8.3.1",
    "@storybook/test": "^8.3.1",
    "@storybook/vue3": "^8.3.1",
    "@storybook/vue3-vite": "^8.3.1",
    "@stylistic/eslint-plugin": "^2.8.0",
    "@tanstack/vue-query-devtools": "^5.56.2",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.16.5",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vitest/eslint-plugin": "^1.1.4",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vue": "^9.28.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "husky": "^9.1.6",
    "jsdom": "^24.1.3",
    "msw": "^2.4.8",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^6.2.3",
    "postcss": "^8.4.47",
    "storybook": "^8.3.1",
    "storybook-vue3-router": "^5.0.0",
    "tailwindcss": "^3.4.11",
    "typescript": "~5.4.5",
    "typescript-eslint": "^8.6.0",
    "vite": "^5.4.6",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.1.6"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "packageManager": "pnpm@9.15.0+sha512.76e2379760a4328ec4415815bcd6628dee727af3779aaa4c914e3944156c4299921a89f976381ee107d41f12cfa4b66681ca9c718f0668fa0831ed4c6d8ba56c"
}
