{
  "name": "vue-setup",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "preview": "vite preview --host",
    "preview:local": "vite preview",
    "build": "vite build",
    "build:sb-scss": "sass ./src/assets/styles/main.scss .storybook/main.css",
    "build:sb-tailwind": "pnpm build:sb-scss && pnpm exec tailwindcss -i .storybook/main.css -o .storybook/compiled.css --minify",
    "build:sb": "pnpm build:sb-tailwind && storybook build",
    "check:type": "vue-tsc --noEmit -p tsconfig.vitest.json",
    "check:lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --max-warnings=0",
    "check:format": "prettier --check .",
    "check": "pnpm check:type && pnpm check:lint && pnpm check:format",
    "dev:local": "vite",
    "dev": "vite --host",
    "dev:sb-scss": "pnpm build:sb-scss --watch",
    "dev:sb-tailwind": "pnpm build:sb-tailwind --watch",
    "dev:sb-styles": "conc \"pnpm dev:sb-scss\" \"pnpm dev:sb-tailwind\"",
    "dev:sb": "conc \"storybook dev -p 6006 --no-open\" \"pnpm dev:sb-styles\"",
    "format": "prettier --write .",
    "format:silent": "pnpm format --log-level silent",
    "test": "vitest"
  },
  "dependencies": {
    "@sentry/vue": "^7.112.2",
    "@tanstack/vue-query": "^5.32.0",
    "@vueuse/components": "^10.9.0",
    "@vueuse/core": "^10.9.0",
    "axios": "^1.6.8",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "1.3.3",
    "@faker-js/faker": "^8.4.1",
    "@pinia/testing": "^0.1.3",
    "@rushstack/eslint-patch": "^1.8.0",
    "@storybook/addon-essentials": "^8.0.9",
    "@storybook/addon-interactions": "^8.0.9",
    "@storybook/addon-links": "^8.0.9",
    "@storybook/blocks": "^8.0.9",
    "@storybook/test": "^8.0.9",
    "@storybook/vue3": "^8.0.9",
    "@storybook/vue3-vite": "^8.0.9",
    "@tanstack/vue-query-devtools": "^5.45.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/vue": "^8.0.3",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.5",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vue": "^9.23.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "msw": "^2.2.14",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "sass": "^1.75.0",
    "storybook": "^8.0.9",
    "storybook-vue3-router": "^5.0.0",
    "tailwindcss": "^3.4.3",
    "typescript": "~5.4.0",
    "vite": "^5.2.8",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.11"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
